/ {
	#address-cells = <2>;
	#size-cells = <2>;
	model = "hi3660";
	compatible = "hisilicon,hi3660";
	interrupt-parent = <&gic>;
	hisi,boardid = <5 5 8 0>;
	hisi,boardname = "VICTORIA_TUGL_V1";
	hisi,modem_id = <0x3A020000>;

	hisi,sub_platform_info3 = "Kirin960";
	hisi,provider3 = "BALONG";
	hisi,sub_platform_info2 = "Kirin960";
	hisi,provider2 = "BALONG";
	hisi,sub_platform_info1 = "Kirin960";
	hisi,provider1 = "BALONG";
	hisi,sub_platform_info0 = "Kirin960";
	hisi,provider0 = "HUAWEI_K3";

	hisi,platform_num = <4>;

	chosen{};

	cpus {
		status = "ok";
	};

        psci {
                compatible      = "arm,psci";
                method          = "smc";
                cpu_suspend     = <0xc4000001>;
                cpu_off         = <0x84000002>;
                cpu_on          = <0xc4000003>;
                system_off      = <0x84000008>;
                system_reset    = <0x84000009>;
        };

	/* cpufreq  device */
	cpufreq: cpufreq-bL {
		compatible = "arm,generic-bL-cpufreq";
		status = "ok";
	};

	hisi_peripheral_tm: hisi_tm@1 {
		compatible = "hisi,hisi-peripheral-tm";
		hisi,peripheral_sensors = <8>;

		hisi,detect_system_h_tm_chanel = <12>;
		hisi,detect_system_h_tm_ntc = "austin_hkadc_soc_temp_table";
		hisi,detect_system_h_tm_state = "enable";

		hisi,detect_flash_led_tm_chanel = <5>;
		hisi,detect_flash_led_tm_ntc = "austin_hkadc_soc_temp_table";
		hisi,detect_flash_led_tm_state = "enable";

		hisi,detect_charger_tm_chanel = <13>;
		hisi,detect_charger_tm_ntc = "austin_hkadc_soc_temp_table";
		hisi,detect_charger_tm_state = "enable";

		hisi,detect_pa_0_tm_chanel = <11>;
		hisi,detect_pa_0_tm_ntc = "austin_hkadc_pa_temp_table";
		hisi,detect_pa_0_tm_state = "enable";

		hisi,detect_pa_1_tm_chanel = <13>;
		hisi,detect_pa_1_tm_ntc = "austin_hkadc_pa_temp_table";
		hisi,detect_pa_1_tm_state = "disable";

		hisi,detect_dcxo0_tm_chanel = <3>;
		hisi,detect_dcxo0_tm_ntc = "austin_hkadc_adc_temp_table";
		hisi,detect_dcxo0_tm_state = "enable";

		hisi,detect_shell_tm_chanel = <7>;
		hisi,detect_shell_tm_ntc = "austin_hkadc_soc_temp_table";
		hisi,detect_shell_tm_state = "disable";

		hisi,detect_charger1_tm_chanel = <4>;
		hisi,detect_charger1_tm_ntc = "austin_hkadc_soc_temp_table";
		hisi,detect_charger1_tm_state = "disable";
	};
    hisi_shell: hisi_shell@2 {
        compatible = "hisi,shell-temp";
        interval = <5000>;
        bias = <94520>;
        count = <6>;
        status = "ok";
        sensors {
            sensor0 {
                type = "system_h";
                coef = "4797","24314","23728","19377","16468","6529";
            };
            sensor1 {
                type = "charger";
                coef = "0","2829","5059","4054","3680","5025";
            };
            sensor2 {
                type = "pa_0";
                coef = "22826","14544","9325","8292","8056","14017";
            };
            sensor3 {
                type = "dcxo0";
                coef = "0","-24143","-18706","-19780","-19364","-19867";
            };
        };
    };

	target-cpu {
		compatible = "hisi,targetcpu";
		target_cpu = "Kirin 960";
	};
	antenna_boardid_detect:antenna_boardid_detect{
		compatible = "huawei,antenna_boardid_detect";
		gpio1 = <&gpio5 3 0>;
		temp_gpio = "gpio1";
		expect_value = <1>;
		pinctrl-names = "default", "idle";
		pinctrl-0 = <&gpio043_cfg_func &gpio043_pmx_func>;
		pinctrl-1 = <&gpio043_cfg_idle &gpio043_pmx_idle>;
		status = "ok";
	};
	fastboot_fsa9685: fastboot_fsa9685{
		compatible = "fastboot,fsa9685";
		chip_i2c_num = <6>;
		status = "ok";
	};
	fastboot_battery:fastboot_battery {
		iin_limit_usb = <470>;
		status = "ok";
	};
	fastboot_charger:fastboot_charger {
		hw_charger_type = <4>;
		chip_i2c_num = <6>;
		status = "ok";
	};
	fastboot_buckboost: fastboot_buckboost{
		hw_buckboost_type = <0>;
		status = "ok";
	};
	hisi_battery:hisi_battery {
		status = "ok";
		batt_name = <&desayatl_3100_39k_4400_25 &scudlgc_3100_200k_4400_25 &sunwodasony_3100_110k_4400_40>;
	};
	hisi_bci_battery:battery {
		battery_design_fcc = <3200>;
		battery_board_type = <2>; /*0:sft 1:udp 2:asic*/
		status = "ok";
	};
	coul_core:coul_core {
		adc_batt_id = <6>;
		adc_batt_temp = <10>;
		r_coul_mohm = <5>;
		last_soc_enable = <1>;
		soc_at_term = <98>;
		 basp_policy = <
			3 50000 6  200 6
			2 40000 8  50  8
			1 30000 9  0   9
			0 0     10 0   10
			>;
		status = "ok";
	};
	huawei_cpu_buck: huawei_cpu_buck{
		status = "ok";
	};
	huawei_hi6422v200_pmu1: huawei_hi6422v200_pmu1 {
		status = "ok";
	};
	huawei_hi6422v200_pmu2: huawei_hi6422v200_pmu2 {
		status = "ok";
	};

	pmic:pmic@FFF34000{
		gpios = <&gpio27 6 0>;
		hisilicon,pmic_fpga_flag=<0>;
		pmic_led: pmic_led@a0 {
			status = "ok";
		};
		pmic_rtc: pmic_rtc@a0 {
			status = "ok";
		};
		ponkey:ponkey@b1{
			lcd_on_off_timeout = <10000>;
			status = "ok";
		};
		hi6421v500_coul: hi6421v500_coul@1 {
			status = "ok";
		};
		huawei_usbvbus {
			support_pd = <1>;
			status = "ok";
		};
        pmic_mntn: pmic_mntn@a0 {
            status = "ok";
            OCP@a3 {
                compatible = "hisilicon,pmic-mntn-ocp-reg0x126";
                hisilicon,inacceptable-event = <0x34>;
                hisilicon,event-bit-name = "ldo20","ldo19","ldo17","ldo16","ldo15","ldo14","ldo13","ldo12";
                hisilicon,event-ops-reg = <0x7E 0x7C 0x7A 0x78 0x76 0x74 0x72 0x70>;
            };
            OCP@a4 {
                compatible = "hisilicon,pmic-mntn-ocp-reg0x127";
                hisilicon,inacceptable-event = <0xF7>;
                hisilicon,event-bit-name = "ldo28","ldo27","ldo26","ldo25","ldo24","ldo23","ldo22","ldo21";
                hisilicon,event-ops-reg = <0x8E 0x8C 0x8A 0x88 0x86 0x84 0x82 0x80>;
            };
            OCP@a5 {
                compatible = "hisilicon,pmic-mntn-ocp-reg0x128";
                hisilicon,inacceptable-event = <0xC1>;
                hisilicon,event-bit-name = "classd","reserved","reserved","reserved","ldo32","ldo31","ldo30","ldo29";
                hisilicon,event-ops-reg = <0xA6 0xDEAD 0xDEAD 0xDEAD 0x96 0x94 0x92 0x90>;
            };

        };
	};
	hisi_coul_drv_test:hisi_coul_test {
		status = "ok";
	};
	huawei_charger: huawei_charger {
		pd_support = <1>;
		powerdown_charge_version = "2";
		support_usb_nonstandard_ico = <1>;
		support_standard_ico = <0>;
		ico_all_the_way = <0>;
		water_check_enabled = <1>;
		fcp_vindpm = <8100>;
		status = "ok";
	};
	vbat_ovp: vbat_ovp {
		status = "ok";
	};
	charging_core: charging_core{
		status = "ok";
		iterm = <124>;
		iin_usb = <470>;
		ichg_usb = <500>;
		ichg_fcp = <3200>;
		iin_fcp = <2000>;
		ichg_ac = <2100>;
		iin_ac = <2000>;
		inductance_para =
			"-32767", "15", "1900", "0",
			"15", "32767", "1900", "0";
	};
	direct_charger: direct_charger{
		use_5A = <1>;
		max_dc_bat_vol = <4350>;
		min_dc_bat_vol = <3500>;
		volt_para =
			"4000", "4150", "3250",
			"4200", "3250", "2250",
			"4380", "2250", "1000";
		compensate_r = <30>;
		cc_cable_detect_enable = <1>;
		ovp_en = <&gpio4 1 0>;
		scp_power_en = <&gpio2 4 0>;
		full_path_res_max = <140>;
		standard_cable_full_path_res_max = <200>;
		temp_para =
			"-32767", "10", "0",
			"10", "45", "4150",
			"45", "32767", "0";
		basp_policy = <
			3	200	6
			2	40	8
			1	20	9
			0	0	10 >;
		status = "ok";
	};
	huawei_smpl: huawei_smpl{
		status = "ok";
	};
	usb_short_circuit_protect: usb_short_circuit_protect{
		status = "ok";
	};

	hisi_vibrator@fff34000 {
		status = "disable";
	};

	fastboot {
		compatible = "hisilicon,fastboot";
		i2c3-pin = <0>;
		gpio-keyup = <205>;
		gpio-keydown = <202>;
