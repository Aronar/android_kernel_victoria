			status = "disabled";
			reset-enable-flag = <1>;
		};

		serial2: uart@fdf03000 {
			pinctrl-names = "default", "idle";
			pinctrl-0 = <>;
			pinctrl-1 = <>;
			clock-rate = <0 160000000>;
			status = "ok";
			reset-enable-flag = <1>;
		};

		serial3: uart@ffd74000 {
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio057_pmx_func &gpio058_pmx_func &gpio059_pmx_func &gpio060_pmx_func &gpio057_cfg_func &gpio058_cfg_func &gpio059_cfg_func &gpio060_cfg_func>;
			pinctrl-1 = <&gpio057_pmx_idle &gpio058_pmx_idle &gpio059_pmx_idle &gpio060_pmx_idle &gpio057_cfg_idle &gpio058_cfg_idle &gpio059_cfg_idle &gpio060_cfg_idle>;
			dma-names = "", "";
			dmas = <>;
			clock-rate = <0 100000000>;
			status = "ok";
			reset-enable-flag = <0>;
		};

		serial4: uart@fdf01000 {
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio061_pmx_func &gpio062_pmx_func &gpio063_pmx_func &gpio064_pmx_func &gpio061_cfg_func &gpio062_cfg_func &gpio063_cfg_func &gpio064_cfg_func>;
			pinctrl-1 = <&gpio061_pmx_idle &gpio062_pmx_idle &gpio063_pmx_idle &gpio064_pmx_idle &gpio061_cfg_idle &gpio062_cfg_idle &gpio063_cfg_idle &gpio064_cfg_idle>;
